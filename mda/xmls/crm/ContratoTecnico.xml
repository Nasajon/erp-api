<?xml version="1.0" encoding="UTF-8"?>

<root xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="apinasajon.xsd">
	<models sistema="crm" modulo="ContratoTecnico">
		<model>
		    <nome>Novo</nome>
            <field nome="Identificador" controlType="Guid" required="False" notnull="False"/>
			<field nome="GrupoEmpresarial" controlType="EntidadeInterna" required="False" notnull="False"/>
			<field nome="Empresa" controlType="EntidadeInterna" required="False" notnull="False"/>
			<field nome="Estabelecimento" controlType="EntidadeInterna" required="True" notnull="True"/>
			<field nome="Negocio" controlType="EntidadeInterna" required="True" notnull="True"/>
			<field nome="Cliente" controlType="EntidadeInterna" required="True" notnull="True"/>
			<field nome="Codigo" controlType="String" tamanho="60" required="True" notnull="True"/>
			<field nome="Descricao" controlType="String" tamanho="255" required="True" notnull="True"/>
			<field nome="InicioVigencia" controlType="Date" required="True" notnull="True"/>	
			<field nome="FimVigencia" controlType="Date" required="False" notnull="False"/>	
			<field nome="Ativo" controlType="Boolean" required="True" notnull="True"/>
		</model>
	</models>
</root>



<?xml version="1.0" encoding="UTF-8"?>

<root xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="apinasajon.xsd">
	<models sistema="servicos" modulo="Contrato">
		<model>
			<field nome="Id" controlType="Guid"/>	
			<field nome="Codigo" controlType="String" tamanho="30"/>	
			<field nome="Descricao" controlType="String" tamanho="120"/> 
			<field nome="TipoContrato" controlType="Integer"/>	
			<field nome="DefinicaoContratante" controlType="String" tamanho="20"/>	
			<field nome="DefinicaoBeneficiario" controlType="String" tamanho="20"/>	
			<field nome="DataInicial" controlType="Date"/>	 
			<field nome="Administrador" controlType="EntidadeInterna"/> 
			<field nome="Estabelecimento" controlType="EntidadeJuridica"/>
			<field nome="Pessoa" controlType="EntidadeFisicaJuridica"/>
			<field nome="Fiador" controlType="EntidadeFisicaJuridica"/>

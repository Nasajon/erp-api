			<field nome="EmitirNotaFiscal" controlType="Boolean"/>	
			<field nome="TipoContabilizacao" controlType="Integer"/>	
			<field nome="Autorizado" controlType="Integer"/>	
			<field nome="Cancelado" controlType="Boolean"/>  
			<field nome="DataHoraCancelamento" controlType="DateTime"/>  
			<field nome="TipoFuncao" controlType="Integer"/> 
			<field nome="Itens" controlType="ObjectList" namespace="servicos\ItemContrato\TItemContrato" /> 

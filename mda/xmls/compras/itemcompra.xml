<?xml version="1.0" encoding="UTF-8"?>

<root xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="apinasajon.xsd">
	<models sistema="compras" modulo="ItemCompra">
		<model>
			<nome>Novo</nome>
			<field nome="id" controlType="Guid" required="False" notnull="True"/>
			<field nome="codigo" controlType="String" tamanho="30" required="True" notnull="True"/>
			<field nome="descricao" controlType="String" tamanho="150" />
			<field nome="comodato" controlType="Boolean" />
            <field nome="tipo" controlType="Integer" />
            <field nome="categoriadeproduto" controlType="EntidadeInterna" required="False" notnull="False"/>
            <field nome="categoriadeservico" controlType="EntidadeInterna" required="False" notnull="False"/>
            <field nome="detalhamento" controlType="text" />
            <field nome="grupoempresarial" controlType="EntidadeInterna" required="True" notnull="True"/>
		</model>
		<model>
			<nome>Alterar</nome>
			<field nome="id" controlType="Guid" required="False" notnull="True"/>
            <field nome="codigo" controlType="String" tamanho="30" required="True" notnull="True"/>
			<field nome="descricao" controlType="String" tamanho="150" />
			<field nome="comodato" controlType="Boolean" />
            <field nome="tipo" controlType="Integer" />
            <field nome="categoriadeproduto" controlType="EntidadeInterna" required="False" notnull="False"/>
            <field nome="categoriadeservico" controlType="EntidadeInterna" required="False" notnull="False"/>
            <field nome="detalhamento" controlType="text" />
            <field nome="grupoempresarial" controlType="EntidadeInterna" required="True" notnull="True"/>
		</model>	
		<model>
			<nome>Excluir</nome>			
			<field nome="id" controlType="Guid" required="True" notnull="True"/>
		</model>
	</models>
</root>
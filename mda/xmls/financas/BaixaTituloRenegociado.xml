			<field nome="Origem" controlType="Integer" Required="False" notnull="False"/>			
		</model>	
		<model>
		    <nome>Excluir</nome>        
			<field nome="IdTitulo" controlType="Guid" notnull="True"/>	
		</model>	        
	</models>
</root>
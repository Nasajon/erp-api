<?xml version="1.0" encoding="UTF-8"?>

<root xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="apinasajon.xsd">
	<models sistema="persona" modulo="Departamento">
		<model>
			<nome>Novo</nome>
			<field nome="IdDepartamento" controlType="Guid" required="False" notnull="True"/>			
			<field nome="Estabelecimento" controlType="EntidadeFisicaJuridica" required="False" notnull="True"/>
			<field nome="Empresa" controlType="EntidadeFisicaJuridica" required="False" notnull="True"/>
			<field nome="Codigo" controlType="String" tamanho="8" required="True" notnull="True"/>
			<field nome="Nome" controlType="String" tamanho="40" required="True" notnull="True"/>
		</model>	
		<model>
			<nome>Alterar</nome>			
			<field nome="Departamento" controlType="EntidadeInterna" required="True" notnull="True"/>
			<field nome="Estabelecimento" controlType="EntidadeFisicaJuridica" required="False" notnull="True"/>
			<field nome="Empresa" controlType="EntidadeFisicaJuridica" required="False" notnull="True"/>
			<field nome="Codigo" controlType="String" tamanho="8" required="False" notnull="True"/>
			<field nome="Nome" controlType="String" tamanho="40" required="False" notnull="True"/>
		</model>	
		<model>
			<nome>Excluir</nome>			
			<field nome="Departamento" controlType="EntidadeInterna" required="True" notnull="True"/>
			<field nome="Estabelecimento" controlType="EntidadeFisicaJuridica" required="False" notnull="True"/>
			<field nome="Empresa" controlType="EntidadeFisicaJuridica" required="False" notnull="True"/>			
		</model>
	</models>
</root>
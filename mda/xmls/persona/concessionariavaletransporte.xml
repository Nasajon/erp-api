			<field nome="Nome" controlType="String" tamanho="40" required="False" notnull="True"/>
		</model>	
		<model>
			<nome>Excluir</nome>			
	        <field nome="Concessionaria" controlType="EntidadeInterna" required="True" notnull="True"/>
		</model>
	</models>
</root>
   
			<field nome="Nome" controlType="String" tamanho="40" required="True" notnull="True"/>
		</model>	
		<model>
			<nome>Alterar</nome>			
		    <field nome="Concessionaria" controlType="EntidadeInterna" required="True" notnull="True"/>
